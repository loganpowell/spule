{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc"
    ]
  },
  "source": {
    "include": [
      "src",
      "package.json",
      "README.md"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "applicationName": "spule",
    "disqus": "",
    "googleAnalytics": "",
    "favicon": "",
    "openGraph": {
      "title": "spule.js",
      "type": "website",
      "image": "",
      "site_name": "spule",
      "url": "https://github.com/loganpowell/spule"
    },
    "meta": {
      "title": "spule",
      "description": "be cool and spule",
      "keyword": "multiplex"
    },
    "linenums": true,
    "source": {
      "include": [
        "./src/"
      ],
      "includePattern": ".+\\.js(doc)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
      "encoding": "utf8",
      "recurse": true,
      "private": false,
      "lenient": true,
      "destination": "./docs",
      "template": "./node_modules/@pixi/jsdoc-template"
    }
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/@pixi/jsdoc-template"
  }
}