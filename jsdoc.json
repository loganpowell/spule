{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc"
    ]
  },
  "source": {
    "include": [
      "src",
      "package.json",
      "README.md"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "template": {
    "title": "spule",
    "shortTitle": "spule",
    "logo": "",
    "links": [ 
      {"github": "https://github.com/loganpowell/spule", "title": "View it on Github"},
      {"npm": "https://www.npmjs.com/package/spule", "title": "View it on NPM", "size": 19}
    ],
    "scheme": "0", 
    "disableSort": false,
    "font": {
      "titleFont": "Roboto", 
      "textFont": "Roboto", 
      "codeFont": "Roboto Mono" 
    }
  },
  "styleSwitcher": true, 
  "colors": [ 
    {
      "text": {
        "primary": "#dcdcdc",
        "contrast": "#f78200"
      },
      "background": {
        "primary": "#282c34",
        "contrast": "#1d2027"
      }
    },
    {
      "text": {
        "primary": "#dcdcdc",
        "contrast": "#5ebd3e"
      },
      "background": {
        "primary": "#282c34",
        "contrast": "#1d2027"
      }
    },
    {
      "text": {
        "primary": "#dcdcdc",
        "contrast": "#ffb900"
      },
      "background": {
        "primary": "#282c34",
        "contrast": "#1d2027"
      }
    },
    {
      "text": {
        "primary": "#dcdcdc",
        "contrast": "#e23838"
      },
      "background": {
        "primary": "#282c34",
        "contrast": "#1d2027"
      }
    },
    {
      "text": {
        "primary": "#dcdcdc",
        "contrast": "#009cdf"
      },
      "background": {
        "primary": "#282c34",
        "contrast": "#1d2027"
      }
    }
  ],
  "fonts": {
    "text": "Fira+Sans|IBM+Plex+Sans|Questrial|Rubik|Source+Sans+Pro|Poppins",
    "code": "Cousine|Cutive+Mono|IBM+Plex+Mono|Fira+Code"
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": false,
    "template": "./node_modules/jsdoc-chameleon-template-spule",
    "recurse": true
  }
}