{
  "name": "spule",
  "author": "Logan Powell",
  "license": "MIT",
  "version": "0.2.25",
  "description": "a state management library for single page applications",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": "https://github.com/loganpowell/spule",
  "homepage": "https://github.com/loganpowell/spule",
  "scripts": {
    "madge": "madge --circular lib/",
    "clean": "rm -rf .cache build dist",
    "start": "parcel examples/integration/index.html --global md -p 8080 --open -d dist watch",
    "build": "npm run clean && cross-env NODE_ENV=production parcel build examples/integration/index.html -d spule --no-source-maps --no-cache --public-url ./",
    "types": "tsc --project tsconfig.build.json",
    "tests": "npm run types && jest",
    "pages": "npm run build && gh-pages -d spule",
    "patch": "npm version patch && npm publish && npm run docs && npm run pages",
    "jsdoc": "jsdoc --configure jsdoc.json --verbose",
    "docs": "npm run types && npm run jsdoc",
    "git": "git add . && git commit -m",
    "postgit": "git push origin master && npm run patch"
  },
  "keywords": [
    "thi.ng",
    "multiplex",
    "FRP"
  ],
  "dependencies": {
    "@thi.ng/rstream": "^3.0.2",
    "@thi.ng/transducers-hdom": "^2.0.34",
    "querystring": "^0.2.0",
    "spule": "^0.2.18",
    "styled-system-hdom": "^0.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/runtime-corejs3": "^7.8.4",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "cross-env": "^7.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "gh-pages": "^2.2.0",
    "jest": "^25.1.0",
    "tsd-jsdoc": "^2.4.0"
  },
  "prettier": {
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": false,
    "singleQuote": false,
    "tabWidth": 2
  }
}
