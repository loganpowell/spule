{
  "name": "spule",
  "author": "Logan Powell",
  "license": "MIT",
  "version": "0.2.15",
  "description": "a state management library for single page applications",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": "https://github.com/loganpowell/spule",
  "homepage": "https://github.com/loganpowell/spule",
  "scripts": {
    "clean": "rm -rf .cache build dist",
    "build": "npm run clean && cross-env NODE_ENV=production parcel build src/index.js --no-source-maps --no-cache --public-url ./",
    "start": "npm run clean && parcel src/index.js --global md -p 8080 --open -d dist watch",
    "docs": "jsdoc --configure jsdoc.json --verbose",
    "types": "jsdoc -t node_modules/tsd-jsdoc/dist -r .",
    "madge": "madge --circular lib/",
    "pages": "gh-pages -d spule"
  },
  "keywords": [
    "thi.ng",
    "multiplex",
    "FRP"
  ],
  "dependencies": {
    "@thi.ng/rstream": "^3.0.2",
    "@thi.ng/transducers-hdom": "^2.0.34",
    "querystring": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/runtime-corejs3": "^7.8.4",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "gh-pages": "^2.2.0",
    "jest": "^25.1.0",
    "tsd-jsdoc": "^2.4.0"
  },
  "prettier": {
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": false,
    "singleQuote": false,
    "tabWidth": 2
  }
}
